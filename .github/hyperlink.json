{
  "problemMatcher": [
    {
      "owner": "hyperlink",
      "pattern": [
        {
          "regexp": "^(WARNING|ERROR):(.+)$",
          "message": 1
        },
        {
          "regexp": "^  source may be at (.+)$",
          "file": 1,
          "loop": true
        }
      ]
    }
  ]
}
