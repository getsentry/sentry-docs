{
  "problemMatcher": [
    {
      "owner": "hyperlink",
      "pattern": [
        {
          "regexp": "^fromPath is (.+)$",
          "fromPath": 1
        },
        {
          "regexp": "^(WARNING|ERROR): (.+) at ([^ ].+) \\(found in (.+)\\)$",
          "message": 1,
          "file": 2
        }
      ]
    }
  ]
}
