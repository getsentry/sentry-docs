    "js-cookie": "^3.0.5",
    "js-yaml": "^4.1.0",
    "mdx-bundler": "^10.0.1",
    "micromark": "^4.0.0",
    "next": "14.0.2",
    "platformicons": "^5.9.2",
    "prism-sentry": "^1.0.2",
    "prismjs": "^1.27.0",
    "query-string": "^6.13.1",
    "react": "^18",
    "react-bootstrap": "^1.6.7",
    "react-dom": "^18",
    "react-feather": "^2.0.8",
    "react-popper": "^2.2.4",
