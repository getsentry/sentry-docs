    "@next/mdx": "^14.0.1",
    "@sentry-internal/global-search": "^0.5.10",
    "@sentry/nextjs": "^7.89.0",
    "@spotlightjs/spotlight": "^1.2.7",
    "@types/mdx": "^2.0.9",
    "bootstrap": "4.6.1",
    "esbuild": "^0.19.8",
    "framer-motion": "^10.12.16",
