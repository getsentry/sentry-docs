    "remark-gfm": "^4.0.1",
    "remark-link-rewrite": "^1.0.7",
    "remark-mdx-images": "^3.0.0",
    "remark-parse": "^11.0.0",
    "remark-prism": "^1.3.6",
    "remark-rehype": "^11.1.2",
    "remark-stringify": "^11.0.0",
    "rss": "^1.2.2",
    "sass": "^1.69.5",
    "search-insights": "^2.17.2",
    "server-only": "^0.0.1",
    "sharp": "^0.33.4",
    "shimmer": "^1.2.1",
    "svg-pan-zoom": "^3.6.2",
    "tailwindcss-scoped-preflight": "^3.0.4",
    "textarea-markdown-editor": "^1.0.4",
