    "framer-motion": "^10.12.16",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "hast-util-heading-rank": "^3.0.0",
    "hast-util-select": "^6.0.4",
    "hast-util-to-jsx-runtime": "^2.3.2",
    "hast-util-to-string": "^3.0.1",
