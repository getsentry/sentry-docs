    "concurrently": "^9.1.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "eslint-config-sentry-docs": "^2.10.0",
    "jest": "^29.5.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.33",
    "postcss-import": "^15.1.0",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^3.0.7",
    "ws": "^8.17.1"
  },
  "pnpm": {
    "overrides": {
      "dompurify": "3.2.4",
      "@types/dompurify": "3.0.5",
      "@types/node": "^22"
    },
    "onlyBuiltDependencies": [
