    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "eslint-config-sentry-docs": "^1.118.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
