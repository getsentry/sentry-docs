{
  "$schema": "https://assets.gtx.dev/config-schema.json",
  "files": {
    "mdx": {
      "include": [
        "./docs/**/*.mdx",
        "./develop-docs/**/*.mdx",
        "./platform-includes/**/*.mdx"
      ],
      "transform": {
        "match": "^(docs|develop-docs|platform-includes)/(.*)$",
        "replace": "$1/{locale}/$2"
      },
      "exclude": [
        "./docs/[locales]/**/*.mdx",
        "./develop-docs/[locales]/**/*.mdx",
        "./platforms-includes/[locales]/**/*.mdx"
      ]
    },
    "gt": {
      "output": "public/_gt/[locale].json"
    }
  },
  "sharedStaticAssets": {
    "include": [
      "./docs/**/*.png",
      "./docs/**/*.jpg",
      "./docs/**/*.gif",
      "./develop-docs/**/*.png",
      "./develop-docs/**/*.jpg"
    ],
    "outDir": "public/assets"
  },
  "defaultLocale": "en",
  "locales": [
    "es",
    "ja"
  ],
  "options": {
    "experimentalLocalizeStaticImports": true,
    "experimentalLocalizeStaticUrls": true,
    "docsHideDefaultLocaleImport": true,
    "experimentalHideDefaultLocale": true,
    "experimentalAddHeaderAnchorIds": "mintlify",
    "experimentalClearLocaleDirs": true
  },
  "modelProvider": "ANTHROPIC",
  "_versionId": "cc6fa71f46a6e283458999668f1c14ccd0d84a15c019ad53f0a409ad71ce4710"
}