{
  "$schema": "https://assets.gtx.dev/config-schema.json",
  "files": {
    "mdx": {
      "include": [
        "./docs/**/*.mdx",
        "./develop-docs/**/*.mdx"
      ],
      "transform": {
        "match": "^(docs|develop-docs)/(.*)$",
        "replace": "$1/{locale}/$2"
      },
      "exclude": [
        "./docs/[locales]/**/*.mdx",
        "./develop-docs/[locales]/**/*.mdx"
      ]
    },
    "gt": {
      "output": "public/_gt/[locale].json"
    }
  },
  "sharedStaticAssets": {
    "include": [
      "./docs/**/*.png",
      "./docs/**/*.jpg",
      "./docs/**/*.gif",
      "./develop-docs/**/*.png",
      "./develop-docs/**/*.jpg"
    ],
    "outDir": "public/assets"
  },
  "defaultLocale": "en",
  "locales": [
    "es"
  ],
  "options": {
    "experimentalLocalizeStaticImports": true,
    "experimentalLocalizeStaticUrls": true,
    "docsHideDefaultLocaleImport": true,
    "experimentalHideDefaultLocale": true,
    "experimentalAddHeaderAnchorIds": "mintlify",
    "experimentalClearLocaleDirs": true
  },
  "_versionId": "859d3bc5f737f358c1465975d999f31308c0bd9abb924ae627717443c3fb3c1a"
}